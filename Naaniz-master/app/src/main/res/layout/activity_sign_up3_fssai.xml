<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUp3Fssai">

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="345dp"
        android:layout_height="346dp"
        android:layout_marginLeft="-125dp"
        android:layout_marginTop="-75dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ellipse_1" />

    <View
        android:id="@+id/rectangle_7"
        android:layout_width="385dp"
        android:layout_height="485dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginTop="64dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_7" />

    <TextView
        android:id="@+id/upload_your"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="36dp"
        android:layout_above="@id/rectangle_7"
        android:layout_marginBottom="-134dp"
        android:text="@string/upload_your_fl"
        android:textAppearance="@style/upload_your"
        android:lineSpacingExtra="0sp"
        android:gravity="top" />

    <View
        android:id="@+id/bar_ill"
        android:layout_width="55dp"
        android:layout_height="4dp"
        android:layout_marginTop="134dp"
        android:layout_marginLeft="36dp"
        android:layout_below="@id/upload_your"
        android:background="@drawable/bar_ill" />

    <ImageView
        android:id="@+id/fssai_doc_image"
        android:layout_width="205dp"
        android:layout_height="268dp"
        android:layout_marginLeft="38dp"
        android:layout_below="@id/upload_your"
        android:background="@drawable/vector_form_outer_boarder"
        android:layout_marginTop="169dp"/>

    <TextView
        android:id="@+id/click_on_th"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38dp"
        android:layout_below="@id/rectangle_7"
        android:layout_marginTop="-64dp"
        android:text="@string/click_on_th"
        android:textAppearance="@style/click_on_th"
        android:lineSpacingExtra="0sp"
        android:gravity="top" />


    <Button
        android:id="@+id/fssai_upload_btn"
        android:layout_width="241dp"
        android:layout_height="60dp"
        android:layout_marginLeft="85dp"
        android:layout_marginRight="85dp"
        android:text="@string/upload"
        android:textAppearance="@style/upload"
        android:lineSpacingExtra="0sp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="36dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/rectangle_2" />


    <Button
        android:id="@+id/fssai_next_btn"
        android:layout_width="241dp"
        android:layout_height="60dp"
        android:layout_marginLeft="85dp"
        android:layout_marginRight="85dp"
        android:text="@string/Next"
        android:enabled="false"
        android:visibility="invisible"
        android:textAppearance="@style/upload"
        android:lineSpacingExtra="0sp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="36dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/rectangle_2" />

</RelativeLayout>